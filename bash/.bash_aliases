# Sets new theme and wallpaper
alias walpaper="wal --iterative -i ${WALLPAPER_DIR:-$HOME/Pictures/Wallpapers/} -o ${PALETTE_SCRIPT_PATH:-$HOME/.local/bin/scripts/starship_py_palette.sh}"
# Sets new theme 
alias waltheme="wal --iterative -n -i ${DESIGN_DIR:-$HOME/Pictures/Design/} -o ${PALETTE_SCRIPT_PATH:-$HOME/.local/bin/scripts/starship_py_palette.sh}"
