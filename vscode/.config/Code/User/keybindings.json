// Place your key bindings in this file to override the defaults
[
  {
    "key": "alt+,",
    "command": "cursorHome",
    "when": "textInputFocus"
  },
  {
    "key": "home",
    "command": "-cursorHome",
    "when": "textInputFocus"
  },
  {
    "key": "alt+.",
    "command": "cursorEnd",
    "when": "textInputFocus"
  },
  {
    "key": "end",
    "command": "-cursorEnd",
    "when": "textInputFocus"
  },
  {
    "key": "alt+w",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  }
]